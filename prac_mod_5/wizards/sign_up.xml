<odoo>
    <record id="signup_wizard_view_form" model="ir.ui.view">
        <field name="name">signup.wizard.view.form</field>
        <field name="model">signup.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="code"/>
                    </group>
                </sheet>
                <footer>
                    <button name="sign_up" string="Sign up" type="object"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>